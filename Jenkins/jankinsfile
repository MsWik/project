    agent any
    stages {
        stage('git') {
            steps{
                git branch: 'main', url: 'https://github.com/MsWik/project'
            }
            
        }
        stage('Execute Ansible') {
            steps{
               ansiblePlaybook credentialsId: 'df2d4b2a-6344-495e-aba4-7ca80eb056b4', installation: 'ansible', inventory: 'hosts.yaml', playbook: 'redmine.yaml'
            }
            
        }
    }
}