pipeline {
    agent any
    options {
        buildDiscarder(logRotator(numToKeepStr:'5'))
        ansiColor('xterm')
    }
    stages {
        stage('git') {
            steps{
                git branch: 'main', url: 'https://github.com/MsWik/project'
            }
            
        }
        stage('Execute Ansible') {
            steps{
				 ansiblePlaybook credentialsId: '49c05a2c-22b6-493f-931c-46aeab8ead57', installation: 'ansible2', inventory: 'hosts.yaml', playbook: 'redmine.yaml', sudo: true

            }
            
        }
    }
}